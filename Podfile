# platform :ios, '9.0'

def shared_pods
    pod 'Alamofire', '~> 4.5'
    pod 'SDWebImage', '~> 4.0'
    pod 'AlamofireObjectMapper', '~> 4.0'
    pod 'Koloda', '~> 4.3.1'
end


target 'FanUno' do
# Comment the next line if you're not using Swift and don't want to use dynamic frameworks
use_frameworks!
    # Pods for FanUno
    shared_pods
end


target 'FanUnoTests' do
    # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
    use_frameworks!
    shared_pods
end
